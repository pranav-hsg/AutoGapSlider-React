{
  "name": "autogapslider",
  "version": "1.0.9",
  "description": "React slider which gives gap automatically between slides according to viewport.",
  "author": "pranav",
  "license": "MIT",
  "repository": "https://github.com/Pranav-medit/AutoGapSlider-React",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "keywords": [
    "react",
    "responsive",
    "reactjs",
    "slider",
    "react-slider",
    "auto-gap-slider",
    "auto-gap-react-slider",
    "auto-space-slider",
    "carousel",
    "react-carousel"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --minify --no-sourcemap --format modern,cjs  --jsxFragment React.Fragment --copy-files assets:dist/as",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --jsxFragment React.Fragment",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "sass": "^1.57.1",
    "tar": "^6.1.11"
  },
  "microbundle": {
    "entry": "src/index.js",
    "output": "dist",
    "copy": [
      {
        "from": "assets",
        "to": "dist/assets"
      }
    ],
  
    "mangle": {
      "properties": {
        "regex": "^_"
      }
    }
  }
}
